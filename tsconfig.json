{
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"@/*": ["*"]
		},
		"allowImportingTsExtensions": true,
		"noEmit": true
	},
	"extends": "expo/tsconfig.base",
	"include": [
		"**/*.ts",
		"**/*.tsx",
		".expo/types/**/*.ts",
		"expo-env.d.ts",
		"app/(auth)/sign-up.tsx",
		"app/(auth)/sign-in.tsx",
		"utils/auth.ts"
	]
}
